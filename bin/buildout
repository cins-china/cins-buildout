#!/usr/bin/env python

import os

join = os.path.join
base = os.path.dirname(os.path.abspath(os.path.realpath(__file__)))
base = os.path.dirname(base)

import sys
sys.path[0:0] = [
    join(base, 'whls/setuptools-40.6.3-py2.py3-none-any.whl'),
    join(base, 'whls/zc.buildout-2.12.2-py2.py3-none-any.whl'),
]

import zc.buildout.buildout

if __name__ == '__main__':
    sys.exit(zc.buildout.buildout.main())
